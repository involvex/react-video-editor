	"scripts": {
		"dev": "next dev --webpack",
		"build": "next build --webpack",
		"start": "next start",
		"lint": "biome check .",
		"lint:fix": "biome check . --write",
		"migrate:up": "tsx scripts/migrate.ts up",
		"migrate:down": "tsx scripts/migrate.ts down",
		"cleanup-db": "tsx scripts/cleanup-db.ts",
		"format": "biome format . --write",
		"electron": "pnpm electron src/electron/main.js",
		"electron:dev": "concurrently \"pnpm dev\" \"wait-on http://localhost:3000 && cross-env NODE_ENV=development && pnpm electron src/electron/main.js\"",
		"build:electron": "pnpm build && pnpm electron-builder --config.win.target=portable"
	},
